<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.ai.mapper.CategoryItemMapper">

    <select id="selectCategoryItemList" resultType="com.ruoyi.ai.domain.CategoryItem">
        select * from ai_interview_category_item
        where del_flag = 1
        <if test="category != null and category != ''">
            and category = #{category}
        </if>
    </select>
</mapper>
